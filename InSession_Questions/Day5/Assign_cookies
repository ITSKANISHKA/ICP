class Solution {
    public int findContentChildren(int[] g, int[] s) {
        Arrays.sort(g);
        Arrays.sort(s);
        int child=g.length;
        int cookie=s.length;
        //int x=0;
        int i=0;
        int j=0;
        while(i<child && j<cookie){
            if(s[j]>=g[i]){
                i++;
            }j++;
        }return i;
    }
}
