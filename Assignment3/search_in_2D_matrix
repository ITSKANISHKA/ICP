class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        int n=matrix.length;
        int r=0;
        int m=matrix[0].length-1;
        int c=m-1;
        while(r<n && c>=0){
            // int mid=s+(e-s)/2;
            if(matrix[r][c]==target){
                return true;
            }if(matrix[r][c]>target){
                c--; 
            }else{
                r++;
            }
        }return false;
        
    }
}
